<p id="notice"><%= notice %></p>

<h1>Listing Search Params</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Mount</th>
      <th>Flange focal distance</th>
      <th>Sensor size</th>
      <th>Region</th>
      <th>Float</th>
      <th>Work length</th>
      <th>Float</th>
      <th>Work lenght round</th>
      <th>Float</th>
      <th>Xiangyuan</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @search_params.each do |search_param| %>
      <tr>
        <td><%= search_param.name %></td>
        <td><%= search_param.mount %></td>
        <td><%= search_param.flange_focal_distance %></td>
        <td><%= search_param.sensor_size %></td>
        <td><%= search_param.region %></td>
        <td><%= search_param.float %></td>
        <td><%= search_param.work_length %></td>
        <td><%= search_param.float %></td>
        <td><%= search_param.work_lenght_round %></td>
        <td><%= search_param.float %></td>
        <td><%= search_param.xiangyuan %></td>
        <td><%= link_to 'Show', search_param %></td>
        <td><%= link_to 'Edit', edit_search_param_path(search_param) %></td>
        <td><%= link_to 'Destroy', search_param, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

